{
  "name": "Skyslackers",
  "version": "1.0.0",
  "description": "A boilerplate to create JavaScript binary executable files using Node.",
  "bin": "server.js",
  "pkg": {
    "assets": [
      "key.pem",
      "cert.pem",
      "index.html"
    ],
    "targets": [
      "node12-macos-x64",
      "node12-win-x64"
    ]
  },
  "dependencies": {
    "applescript": "^1.0.0",
    "express": "^4.17.1",
    "pkg": "^5.3.0",
    "qrcode": "^1.4.4",
    "socket.io": "^4.1.2"
  }
}
